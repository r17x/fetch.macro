{
  "name": "fetch.macro",
  "description": "write fetch function with babel.macros",
  "author": "r17x <hi@rin.rocks>",
  "homepage": "https://github.com/r17x/fetch.macro",
  "repository": "https://github.com/r17x/fetch.macro",
  "version": "1.0.0",
  "keywords": [
    "babel-plugin-macros",
    "fetch"
  ],
  "main": "src/fetch.macro.js",
  "license": "MIT",
  "scripts": {
    "prepare": "husky install",
    "lint": "eslint",
    "test": "jest"
  },
  "prettier": {
    "endOfLine": "auto",
    "printWidth": 120,
    "semi": true,
    "singleQuote": false,
    "trailingComma": "all"
  },
  "eslintConfig": {
    "extends": [
      "eslint-config-kdnj"
    ],
    "root": true
  },
  "lint-staged": {
    "*": "prettier --ignore-unknown --write",
    "*.js": [
      "eslint --fix",
      "cross-env NODE_ENV=test jest --bail --findRelatedTests --passWithNoTests"
    ]
  },
  "jest": {
    "testMatch": [
      "**/tests/**/*.[jt]s?(x)"
    ],
    "testPathIgnorePatterns": [
      "/node_modules/",
      "tests/create-tests.js"
    ]
  },
  "peerDependencies": {
    "babel-plugin-macros": "^2"
  },
  "devDependencies": {
    "@babel/core": "^7",
    "@commitlint/cli": "16.2.1",
    "@commitlint/config-conventional": "16.2.1",
    "@commitlint/prompt": "16.2.1",
    "@semantic-release/changelog": "semantic-release/changelog",
    "@semantic-release/git": "10.0.1",
    "@semrel-extra/npm": "1.2.0",
    "commitlint-config-squash-pr": "1.1.0",
    "babel-plugin-macros": "^3",
    "babel-plugin-tester": "^10",
    "cross-env": "^7",
    "eslint": "^8",
    "eslint-config-kdnj": "^2",
    "prettier": "^2",
    "husky": "^8",
    "jest": "^29",
    "lint-staged": "^13",
    "semantic-release": "19.0.2"
  }
}
